 tag --}}\n  {{ghost_head}}\n</head>\n<body>\n  <div class=\"wrapper\">\n    <header>\n      <h1 class=\"blog-logo\">\n        {{#if @blog.logo}}\n          <a href=\"{{@blog.url}}\"><img src=\"{{@blog.logo}}\" alt=\"{{@blog.title}}\" /></a>\n        {{else}}\n          <a href=\"{{@blog.url}}\">{{@blog.title}}</a>\n        {{/if}}\n      </h1>\n      <p class=\"blog-desc\">{{@blog.description}}</p>\n\n      {{navigation}}\n\n      <div class=\"sidebar\">\n\n        <div class=\"display form\">\n          <form class=\"search\" method=\"POST\" action=\"#\">\n            <input id=\"search-field\" type=\"text\" name=\"search\" value=\"\" placeholder=\"Search...\"/>\n            <button type=\"submit\">SEARCH</button>\n          </form>\n          <div id=\"results\"></div>\n        </div>\n\n        <div class=\"display\"></div>\n\n        <div class=\"display tags\">\n          <h5 class=\"sidebar-title\">Tags Category</h5>\n          {{#get \"tags\"}}\n            {{#foreach tags}}\n              <a href=\"{{url}}\">{{name}}</a>\n            {{/foreach}}\n          {{/get}}\n        </div>\n\n        <div class=\"display socials\">\n          <h5 class=\"sidebar-title\">Socials</h5>\n          <a href=\"#\" class=\"sb-email\" id=\"B_email\" target=\"_blank\" alt=\"Email\"><i class=\"fa fa-envelope\"></i></a>\n          <a href=\"#\" class=\"sb-facebook\" id=\"B_facebook\" target=\"_blank\" alt=\"Facebook\"><i class=\"fa fa-facebook\"></i></a>\n          <a href=\"#\" class=\"sb-twitter\" id=\"B_twitter\" target=\"_blank\" alt=\"Twitter\"><i class=\"fa fa-twitter\"></i></a>\n          <a href=\"#\" class=\"sb-dribbble\" id=\"B_dribble\" target=\"_blank\" alt=\"Dribbble\"><i class=\"fa fa-dribbble\"></i></a>\n          <a href=\"#\" class=\"sb-google-plus\" id=\"B_googleplus\" target=\"_blank\" alt=\"Google+\"><i class=\"fa fa-google-plus\"></i> </a>\n          <a href=\"#\" class=\"sb-pinterest\" id=\"B_pinterest\" target=\"_blank\" alt=\"Pinterest\"><i class=\"fa f